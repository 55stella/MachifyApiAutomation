	"item": [
		{
			"name": "Profile Creation",
			"item": [
				{
					"name": "Successful profile creation",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jsonData = pm.response.json();",
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"pm.test('Successful profile creation with valid inputs', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData.user.name).to.equal('John Doe');",
									"    pm.expect(jsonData.user.age).to.equal('30');",
									"    pm.expect(jsonData.user.gender).to.equal('Male');",
									"    pm.expect(jsonData.user).to.have.property('id');",
									"    pm.expect(jsonData.user.interests).to.equals(\"Coding, Music, Traveling\")",
									"})",
									"pm.test('Message indicates Successful profile creation', function () {",
									"    pm.expect(jsonData.message).to.equal('Profile created successfully!');",
									"})",
									"pm.test(\"verify that age must be a number only\", function () {",
									"    const ageToNumber = Number(jsonData.user.age)",
									"    pm.expect(ageToNumber).to.be.a(\"number\");",
									"});",
									"",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"name\": \"John Doe\",\n  \"age\": \"30\",\n  \"gender\": \"Male\",\n  \"location\": \"New York\",\n  \"interests\": \"Coding, Music, Traveling\",\n  \"username\": \"johndoe\",\n  \"password\": \"password123\"\n}\n",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "https://matchify-backend-theta.vercel.app/api/profile",
							"protocol": "https",
							"host": [
								"matchify-backend-theta",
								"vercel",
								"app"
							],
							"path": [
								"api",
								"profile"
							]
						}
					},
					"response": []
				},
